{
  "name": "maana-my-service",
  "author": "Maana, Inc.",
  "license": "MIT",
  "version": "1.0.0",
  "description": "TypeScript Maana Q Knowledge Service Template",
  "main": "dist/main.js",
  "repository": "git@github.com:maana-io/q-template-service-node-ts-basic",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "startdev": "tsc-watch --onSuccess \"node ./dist/main.js\"",
    "start": "node ./dist/main.js",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/uuid": "^8.3.0",
    "class-validator": "^0.13.1",
    "core-js": "^3.8.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^26.5.0",
    "tsc-watch": "^4.2.9",
    "type-graphql": "^1.1.1",
    "typescript": "^3.9.0"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFiles": [
      "core-js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "/src/test/tests/.*\\.(ts|tsx|js)$"
  }
}
